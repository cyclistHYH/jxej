.popu{
  width: 100%;
  height: 100vh;
  background: rgba(0, 0, 0, 0.5);
  position: fixed;
  left: 0;
  top: 0;
  display: flex;
  justify-content: center;
  align-items: center;
  z-index: 999;
}
.popuText{
  width: 400rpx;
  height: 200rpx;
  text-align: center;
  font-size: 32rpx;
  background: #fff;
  line-height: 200rpx;
  border-radius: 10rpx;
  color: #333333;
}