.container {
  width: 100%;
  height: 100vh;
  padding-top: 12rpx;
  padding-bottom: 90rpx;
}

.carouselWrap {
  position: relative;
  width: 100%;
  height: 696rpx;
  overflow: hidden;
}

.carousel {
  width: 100%;
  height: 696rpx;
}

.carouselBox {
  width: 100%;
  height: 100%;
  overflow: hidden;
}

.carouselBox image {
  display: block;
  width: 100%;
  height: 100%;
}

